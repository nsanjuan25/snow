create or replace TABLE ERROR_LOG (
	ERRORKEY NUMBER(38,0) NOT NULL autoincrement,
	PROCEDURENAME VARCHAR(16777216) COLLATE 'en-ci',
	LOGDATE TIMESTAMP_NTZ(9) DEFAULT CURRENT_TIMESTAMP(),
	ERROR_CODE NUMBER(38,0),
	ERROR_STATE VARCHAR(16777216) COLLATE 'en-ci',
	ERROR_MESSAGE VARCHAR(16777216) COLLATE 'en-ci',
	STACK_TRACE VARCHAR(16777216) COLLATE 'en-ci',
	primary key (ERRORKEY)
);